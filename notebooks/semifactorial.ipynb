   "source": [
    "def semifactorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        # return the multiplicative identity\n",
    "        return 1\n",
    "    # return n(n - 2) for all integers between\n",
    "    # n and 1 with the same parity as n\n",
    "    return n * semifactorial(n-2)"
   ]
  },
